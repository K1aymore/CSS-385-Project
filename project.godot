;   param=value ; assign values to parameters

config_version=5

[application]

config/name="CSS 385 Project"
run/main_scene="res://world.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Player="*res://Player/player.tscn"

[display]

window/size/viewport_width=450
window/size/viewport_height=240
window/size/mode=4
window/size/resizable=false
window/size/borderless=true
window/stretch/mode="viewport"
mouse_cursor/custom_image="res://Player/reticle.png"
mouse_cursor/custom_image_hotspot=Vector2(32, 32)

[gui]

fonts/dynamic_fonts/use_oversampling=false
theme/custom_font="res://fonts/tiny.ttf"
theme/default_font_antialiasing=0
theme/default_font_hinting=0

[input]

leanOut={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":32,"key_label":0,"unicode":32,"location":0,"echo":false,"script":null)
]
}

[rendering]

anti_aliasing/screen_space_roughness_limiter/enabled=false
2d/snap/snap_2d_transforms_to_pixel=true
2d/snap/snap_2d_vertices_to_pixel=true
